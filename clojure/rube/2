
def images_of_pieces (moves, pieces):
	images = moves_on_pieces(moves, pieces)
	result = []
	n = len(pieces)
	for i in range(0, n):
		piece = pieces[i]
		image = images[i]
		if (piece_equal(piece, image)):
			sign = sign_of_rotation(piece, image)
			if (sign != []):
				result.append([piece, image])
		else:
			result.append([piece, "->", image])
	return result
